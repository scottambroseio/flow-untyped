{
    "parser": "babel-eslint",
    "extends": [
        "airbnb"
    ],
    "rules": {
        "linebreak-style": [
            "error",
            "unix"
        ],
        "no-console": "off",
        "no-tabs": "off",
        "indent": [
            "error",
            4
        ]
    },
    "env": {
        "jest": true,
        "node": true
    },
    "plugins": [
        "flowtype"
    ]
}
